        "postcss-combine-duplicated-selectors": "^10.0.3",
        "postcss-custom-properties": "^12.1.9",
        "postcss-import": "^15.0.0",
        "postcss-nested": "^6.0.0",
        "postcss-preset-env": "^7.8.2"
    },
